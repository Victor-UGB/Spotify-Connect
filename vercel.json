{
    "version": 2,
    "builds": [
        {
        "src": "Connect/wsgi.py",
        "use": "@vercel/python",
        "config" : {
            "maxLambdaSize": "15mb",
            "runtime": "python3.8"
        }
        },
        {
        "src": "frontend/build",
        "use": "@vercel/static"
        }
    ],
    "routes": [
        {
        "src": "/static/(.*)",
        "dest": "/static/$1"
        },
        {
        "src": "/(.*)",
        "dest": "Connect/wsgi.py"
        }
    ]
}